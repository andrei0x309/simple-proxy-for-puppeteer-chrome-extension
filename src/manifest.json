{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.1.1",
  "version_name": "1.1.1",
  "icons": {
    "16": "src/assets/images/spfp_icon16.png",
    "32": "src/assets/images/spfp_icon32.png",
    "48": "src/assets/images/spfp_icon48.png",
    "128": "src/assets/images/spfp_icon128.png"
  },
  "action": {
    "default_popup": "src/action-page/action-page.html",
    "default_icon": {
      "16": "src/assets/images/spfp_icon16.png",
      "32": "src/assets/images/spfp_icon32.png",
      "48": "src/assets/images/spfp_icon48.png",
      "128": "src/assets/images/spfp_icon128.png"
    },
    "wakeup_page": "src/wakeup-page/wakeup-page.html"
  },
  "minimum_chrome_version": "103",
  "permissions": ["storage", "proxy"],
  "background": {
    "service_worker": "/src/service-worker.ts",
    "type": "module"
  }
}
